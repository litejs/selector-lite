{
  "name": "selector-lite",
  "version": "20.2.0",
  "license": "MIT",
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "description": "A small pure-JavaScript CSS selector engine",
  "keywords": [
    "browser",
    "css",
    "selector",
    "matches",
    "closest",
    "litejs"
  ],
  "files": [],
  "repository": "git://github.com/litejs/selector-lite.git",
  "bugs": "https://github.com/litejs/selector-lite/issues",
  "litejs": {
    "lint": "jshint *.js"
  },
  "jshintConfig": {
    "asi": true,
    "laxcomma": true,
    "maxdepth": 6,
    "quotmark": "double"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  }
}
